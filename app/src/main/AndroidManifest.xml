<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.solthrift">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Solthrift"
        tools:targetApi="31">
        <activity
            android:name=".upload_activity"
            android:exported="false"
            android:theme="@style/Theme.customone"/>
        <activity
            android:name=".adminn_account_login"
            android:exported="false"
            android:theme="@style/Theme.customtwo" />
        <activity
            android:name=".admin_main"
            android:exported="false"
            android:theme="@style/Theme.customthree" />
        <activity
            android:name=".admin_login"
            android:exported="false"
            android:theme="@style/Theme.customtwo" />
        <activity
            android:name=".shopper"
            android:exported="false"
            android:theme="@style/Theme.customtwo" />
        <activity
            android:name=".splash"
            android:exported="true"
            android:theme="@style/Theme.customone">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.customone"/>
    </application>

</manifest>